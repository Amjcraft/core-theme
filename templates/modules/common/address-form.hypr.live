                </option>
                <option {% if model.address.stateOrProvince == "MO" %}selected="selected"{% endif %} value="MO">
                    Missouri
                </option>
                <option {% if model.address.stateOrProvince == "MS" %}selected="selected"{% endif %} value="MS">
                    Mississippi
                </option>
                <option {% if model.address.stateOrProvince == "MT" %}selected="selected"{% endif %} value="MT">
                    Montana
                </option>
                <option {% if model.address.stateOrProvince == "NC" %}selected="selected"{% endif %} value="NC">
