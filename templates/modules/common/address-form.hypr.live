                </option>
                <option {% if model.address.stateOrProvince == "DE" %}selected="selected"{% endif %} value="DE">
                    Delaware
                </option>
                <option {% if model.address.stateOrProvince == "FL" %}selected="selected"{% endif %} value="FL">
                    Florida
                </option>
                <option {% if model.address.stateOrProvince == "GA" %}selected="selected"{% endif %} value="GA">
                    Georgia
                </option>
                <option {% if model.address.stateOrProvince == "HI" %}selected="selected"{% endif %} value="HI">
                    Hawaii
                </option>
                <option {% if model.address.stateOrProvince == "IA" %}selected="selected"{% endif %} value="IA">
                    Iowa
                </option>
                <option {% if model.address.stateOrProvince == "ID" %}selected="selected"{% endif %} value="ID">
                    Idaho
                </option>
                <option {% if model.address.stateOrProvince == "IL" %}selected="selected"{% endif %} value="IL">
                    Illinois
                </option>
                <option {% if model.address.stateOrProvince == "IN" %}selected="selected"{% endif %} value="IN">
                    Indiana
                </option>
                <option {% if model.address.stateOrProvince == "KS" %}selected="selected"{% endif %} value="KS">
