                </option>
                <option {% if model.address.stateOrProvince == "NH" %}selected="selected"{% endif %} value="NH">
                    New Hampshire
                </option>
                <option {% if model.address.stateOrProvince == "NJ" %}selected="selected"{% endif %} value="NJ">
