{% extends "page" %}

{% block title-tag-content %}{% if PageContext.MetaTitle %}{{PageContext.MetaTitle}}{% else %}{{model.Name}}{%endif%}  - {% parent %}{% endblock title-tag-content %}

{% block body-tag-classes %} mz-notfoundpage {% endblock body-tag-classes %}

{% block body-content %}
<div class="mz-l-container">
    <h1 class="mz-pagetitle">{{ labels.pageNotFound }}</h1>
    {% dropzone "notfound" scope="template" %}
</div>
{% endblock body-content %} 

